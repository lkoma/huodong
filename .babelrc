{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": {
                    "browsers": [
                        "Android >= 4",
                        "iOS > 7",
                        "> 5%"
                    ]
                }
            }
        ],
        "stage-0"
    ],
    "plugins": [
        ["syntax-dynamic-import"],
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": true,
                "regenerator": true
            }
        ]
	]
}